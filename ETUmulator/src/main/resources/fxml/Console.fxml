<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.control.TextField?>

<VBox fx:id="vbox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kasirgalabs.etumulator.console.BaseConsole">
    <children>
        <TilePane>
            <Label maxWidth="Infinity" text="Console:" />
            <Button onAction="#clearArea" text="Clear Console" />
            <Button onAction="#setBreakpoint" text="Breakpoint" />
            <TextField fx:id="point" prefWidth="10.0"/>
        </TilePane>
    </children>
</VBox>
